{
  "version": 2,
  "builds": [
    {
      "src": "sr_model/fusion_api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/predict", "dest": "sr_model/fusion_api.py" },
    { "src": "/batch_predict", "dest": "sr_model/fusion_api.py" },
    { "src": "/live_predict/(.*)", "dest": "sr_model/fusion_api.py" },
    { "src": "/", "dest": "sr_model/fusion_api.py" }
  ]
}
