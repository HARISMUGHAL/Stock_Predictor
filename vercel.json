{
  "version": 2,
  "builds": [
    {
      "src": "sr_model/fusion_api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "methods": ["POST"],
      "dest": "sr_model/fusion_api.py"
    },
    {
      "src": "/batch_predict",
      "methods": ["POST"],
      "dest": "sr_model/fusion_api.py"
    },
    {
      "src": "/live_predict/(.*)",
      "methods": ["GET"],
      "dest": "sr_model/fusion_api.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "sr_model/fusion_api.py"
    }
  ]
}
